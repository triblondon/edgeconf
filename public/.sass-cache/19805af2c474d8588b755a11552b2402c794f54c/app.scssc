//Helper mixins
@mixin breakpoint($point) {
  @if $point == mobile {
    @media only screen and (max-width : 768px) { @content; }
  }
}

@mixin prefixes($property, $values) {
	-webkit-#{$property}: $values;
	-moz-#{$property}: $values;
	-ms-#{$property}: $values;
	-o-#{$property}: $values;
	#{$property}: $values;
}

@function em($target, $context: $em-base) {
   @return #{$target / $context}em;
}

//Fonts
@import "fontawesome.scss";


body {
	font-family: 'Lato', sans-serif;
}
h1, h2 ,h3, h4, h5, h6 {	
	color: #333;
	font-family: 'Lato', sans-serif;
	line-height: 1em;
	text-transform: uppercase;
}
a:link, a:visited {
	color: $edgeGreen;
}
a:hover, a:active {
	color: lighten($edgeGreen, 5);
}
a.button {
	background: #333;
	border: none;
	@include prefixes(border-radius, 4px);
	box-shadow: none;
	color: #fff;
	text-transform: uppercase;
}
.button:hover {
	background: $edgeYellow;
	color: #333;
}
p {
	line-height: 1.5em;
}
ul {
	margin-bottom: 30px;
	li {
		font-size: em(18px);
		list-style-type: none;
	}
}
header {
	background: #000;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 3;
	nav {
		ul {
			margin: 0;
			padding: 7px 0;
		}
		li {	
			display: inline-block;
			text-align: center;
			width: 23.65%;
			a:link, a:visited {
				color: #fff;
				font-size: em(12px);
				font-weight: 900;
				text-transform: uppercase;
			}
			a:hover, a:active, a.active {
				color: $edgeGreen;
			}
		}
		i {
			color: #fff;
			display: none !important; /* Needed to override Font Awesome display property */
			font-size: em(24px);
			position: absolute;
			top: 12px;
			right: 5.5%;
		}
	}
	@include breakpoint(mobile) {
		height: 55px;
		nav.row {
			padding: 0;
			ul.columns {
				background: #333;
				display: none;
				padding: 0;
				position: relative;
				top: 55px;
				width: 90%;
			}
			li {
				border-top: 1px solid #ddd;
				display: block;
				padding: 10px;
				text-align: center;
				width: 100%;
				a:link, a:visited {
					display: block;
					font-size: em(24px);
					font-weight: 900;
					text-transform: uppercase;
				}
			}
			li:first-child {
				border-top: none;
			}
			ul:before {
				content: "";
				border: {
					right: 10px solid transparent;
					bottom: 10px solid #333;
					left: 10px solid transparent;
				}
				display: block;
				height: 10px;
				position: absolute;
				right: 0;
				top: -10px;
				width: 10px;
			}
			i {
				display: block !important;
			}
		}
	}
}
#billboard {
	background: #222 url(/images/bg_billboard.jpg) center top no-repeat;
	background-size: cover;
	margin-top: 35px; //Account for fixed header bar
	//min-height: 270px;
	padding: 45px 0 0 0;
	h1 {
		background: url(/images/logo.png) center top no-repeat;
		background-size: 100%;
		min-height: 185px;
	}
	.tagline {
		color: #fff;
		font-size: em(26px);
		margin-top: 55px;
	}
	@include breakpoint(mobile) {
		min-height: 0;
		padding: 40px 0 20px 0;
		h1 {
			min-height: 140px;
		}
	}
}
#billboard.inner {
	padding-top: 15px;
	h1 {
		min-height: 110px;
	}
	h2 {
		color: #fff;
		font-size: em(36px);
		font-weight: 900;
		margin-top: 40px;
	}
}
.layer {
	border: {
		top: none;
		right: none;
		//bottom: 10px solid #333;
		bottom: 2px dotted #bbb;
		left: none;
	}
	padding: 30px 0 30px 0;
	//text-align: center;
	h2 {
		font-size: em(36px);
		font-weight: 900;
		margin-bottom: 10px;
	}
	p {
		font-size: em(18px);
	}
	img.full {
		width: 100%;
		margin: {
			top: 30px;
			//bottom: -32px; // Set photo even with the bottom of the .layer div
		}
	}
	.button {
		margin-bottom: 30px;
	}
	.next-button {
		background: $edgeGreen;
		color: #fff;
		display: block;
		font-size: em(36px);
		font-weight: 300;
		margin-top: 40px;
		opacity: .85;
		padding: 20px 0;
		position: relative;
		text-align: center;
		text-transform: uppercase;
		-webkit-transition: opacity 0.2s ease;
		-moz-transition: opacity 0.2s ease;
		-ms-transition: opacity 0.2s ease;
		-o-transition: opacity 0.2s ease;
		transition: opacity 0.2s ease;
		width: 97%;
		strong {
			font-weight: 900;
		}
	}
	.next-button:after {
		border: {
			top: 38px solid transparent;
			bottom: 38px solid transparent;
			left: 30px solid $edgeGreen;
		}
		content: "";
		display: block;
		position: absolute;
		right: -30px;
		top: 0;
		width: 3%;
	}
	.next-button:hover {
		opacity: 1;
		-webkit-transition: opacity 0.2s ease;
		-moz-transition: opacity 0.2s ease;
		-ms-transition: opacity 0.2s ease;
		-o-transition: opacity 0.2s ease;
		transition: opacity 0.2s ease;
	}
	.large-6 {
		margin-bottom: 20px;
		h2 {
			font-size: em(24px, 16px);
		}
		img {	
			margin-top: 5px;
			width: 100%;
		}
	}
	@include breakpoint(mobile) {
		h2 {
			font-size: em(26px);
		}
		p {
			font-size: em(14px);
		}
		.button {
			width: 100%;
		}
	}
}
.layer.connect {
	border: none;
	.social-icons {
		li {
			.facebook {
				background: url(/images/icon_facebook_dark.png) center top no-repeat;
			}
			.twitter {
				background: url(/images/icon_twitter_dark.png) center top no-repeat;
			}
			.google-plus {
				background: url(/images/icon_googleplus_dark.png) center top no-repeat;
			}
		}
	}
}
#billboard .layer {
	background: rgba(0,0,0, .4);
	border-bottom: none;
	margin-bottom: 0;
	margin-top: 25px;
	padding-bottom: 5px;
	h2, p {
		color: #fff;
		text-align: center;
	}
	p {	
		font-size: em(24px);
		margin-bottom: 45px;
		strong {
			font-weight: 900;
			text-transform: uppercase;
		}
	}
	h2 + p {
		margin-bottom: 20px;
	}
	.row {
		p {
			margin-bottom: 15px;
			text-align: left;
		}
	}
	p a:link, p a:visited {
		color: $edgeBlue;
	}
	p a:hover, p a:active {
		color: lighten($edgeBlue, 5);
	}
	@include breakpoint(mobile) {
		p {
			font-size: em(16px);
		}
	}
}
.layer.panelists {
	border: none;
}
.panelist {
	margin: 20px 0;
	dt {
		font-size: em(18px);
		text-transform: uppercase;
	}
	dt, dd {	
		clear: right;
		float: right;
		margin-bottom: 5px;
		width: 75%;
	}
	dd.headshot {
		clear: left;
		float: left;
		margin-top: 5px;
		width: 20%;
		img {
			opacity: .1;
			-webkit-transition: opacity 0.2s ease;
			-moz-transition: opacity 0.2s ease;
			-ms-transition: opacity 0.2s ease;
			-o-transition: opacity 0.2s ease;
			transition: opacity 0.2s ease;
			width: 100%;
		}
	}
	dd.headshot + dd {
		font-size: em(18px);
		font-style: italic;
	}
}
.panelist:hover {
	dd.headshot img {
		opacity: 1;
		-webkit-transition: opacity 0.2s ease;
		-moz-transition: opacity 0.2s ease;
		-ms-transition: opacity 0.2s ease;
		-o-transition: opacity 0.2s ease;
		transition: opacity 0.2s ease;
	}
}
.layer.final-session {
	border: none;
}
.social-icons {
	li {
		display: block;
		float: left;
		list-style-type: none;
		margin-right: 20px;
		width: 12%;
		a {
			background-size: cover;
			display: block;
			min-height: 55px;
			min-width: 55px;
			opacity: .9;
		}
		a:hover {
			opacity: 1;
		}
		.facebook {
			background: url(/images/icon_facebook.png) center top no-repeat;
		}
		.twitter {
			background: url(/images/icon_twitter.png) center top no-repeat;
		}
		.google-plus {
			background: url(/images/icon_googleplus.png) center top no-repeat;
		}
	}
	@include breakpoint(mobile) {
		li {
			margin-right: 35px;
			a {
				text-indent: 180%;
			}
		}
	}
}
#registration-form {
	/* Override default Foundation styles */
	bottom: 50px;
	overflow: scroll;
	position: fixed;
	top: 50px !important;
	h2 {
		margin-left: 10px;
	}
	iframe {
		max-width: 1000px;
		width: 100%;
	}
	@include breakpoint(mobile) {
		width: 90%;
		left: 5%;
		margin: 0;
		iframe {
			height: 1500px !important;
		}
	}
}
.modal-open {
	overflow: hidden;
}
footer {
	background: #333;
	padding: 10px 0;
	text-align: center;
	width: 100%;
	p {
		color: #fff;
		font-size: em(14px);
		margin-bottom: 0;
	}
	@include breakpoint(mobile) {
		p {
			font-size: em(12px);
		}
	}
}
